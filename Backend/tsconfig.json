{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "Backend/src",
    "outDir": "./dist",
    "typeRoots": ["Backend/node_modules/@types", "Backend/src/types"]
  },
  "include": [
    "Backend/src/**/*"
, "script", "newsfeed/src/components"  ],
  "exclude": [
    "Backend/node_modules",
    "dist"
  ]
}
